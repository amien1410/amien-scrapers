      "source": [
        "from openai import OpenAI\n",
        "\n",
        "client = OpenAI()\n",
        "audio_file= open(\"/path/to/file/audio.mp3\", \"rb\")\n",
        "\n",
        "transcription = client.audio.transcriptions.create(\n",
        "    model=\"gpt-4o-transcribe\",\n",
        "    file=audio_file\n",
        ")\n",
        "\n",
        "print(transcription.text)"
      ]
    }
  ]
